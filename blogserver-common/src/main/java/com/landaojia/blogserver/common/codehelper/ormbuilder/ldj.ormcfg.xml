<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
<!-- 
	<classPathEntry location="/Program Files/IBM/SQLLIB/java/db2java.zip" />
 -->
	<context id="DB2Tables" targetRuntime="MyBatis3">
		<property name="javaFileEncoding" value="utf-8" />
		<plugin type="com.github.abel533.generator.MapperPlugin">
			<property name="mappers" value="com.github.abel533.mapper.Mapper" />
		</plugin>
		<commentGenerator>
			<property name="suppressDate" value="true" />  
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3306/ldj2?useUnicode=true&amp;characterEncoding=UTF-8" userId="root"
			password="1234" />
		<javaModelGenerator targetPackage="com.landaojia.dao.monitor.model" targetProject=".output" />
		<sqlMapGenerator targetPackage="com.landaojia.dao.monitor.mapper" targetProject=".output" />
		<javaClientGenerator targetPackage="com.landaojia.dao.monitor.client" targetProject=".output" type="XMLMAPPER" />
		<table tableName="M_SQL_CONFIG">
			<generatedKey column="id" sqlStatement="Mysql" identity="true" />
		</table>
